<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<type>bar</type>
	<text_size>10</text_size>
	<data_type>xml</data_type>
	<font>Tahoma</font>
	<background>
		<alpha>100</alpha>
		<border_alpha>20</border_alpha>
	</background>
  	<plot_area>
    		<margins>
      		<left>150</left>
    		</margins>
  	</plot_area>
	<grid>
		<category>
			<dashed>1</dashed>
		</category>
		<value>
			<dashed>1</dashed>
		</value>
	</grid>
	<axes>
		<category>
			<width>1</width>
			<color>E7E7E7</color>
		</category>
		<value>
			<width>1</width>
			<color>E7E7E7</color>
		</value>
	</axes>
	<values><value><min>0</min></value></values>
	<depth>15</depth>
	<column>
		<type>stacked</type>
		<width>50</width>
		<balloon_text>{title}: {value}</balloon_text>
		<grow_time>1.7</grow_time>
	</column>
	<labels>
		<label lid='0'>
			<text><![CDATA[<b></b>]]></text>
			<y>18</y><text_color>000000</text_color>
			<text_size>10</text_size><align>center</align>
		</label>
	</labels>
</settings>
